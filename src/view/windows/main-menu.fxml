<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="600.0" prefWidth="1070.0" xmlns="http://javafx.com/javafx/8.0.171"
           fx:controller="view.controllers.MainMenuController">
    <children>
        <AnchorPane id="main-menu-anchor" prefHeight="200.0" prefWidth="200.0">
            <children>
                <Button fx:id="playButton" alignment="CENTER" contentDisplay="CENTER" layoutX="430.0" layoutY="300.0"
                        mnemonicParsing="false" onAction="#playButtonHandle" prefHeight="79.0" prefWidth="210.0"
                        style="-fx-background-color: #FFFFFF;" text="Play">
                    <font>
                        <Font name="System Italic" size="30"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
    <stylesheets>
        <URL value="@../resources/style.css"/>
    </stylesheets>
</StackPane>
